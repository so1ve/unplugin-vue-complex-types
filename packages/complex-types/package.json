{
	"name": "@vue.ts/complex-types",
	"version": "0.4.0",
	"author": "Ray <i@mk1.io> (@so1ve)",
	"description": "Resolve complex types in Vue SFCs.",
	"keywords": [
		"astro",
		"nuxt",
		"rollup",
		"rspack",
		"transform",
		"typescript",
		"unplugin",
		"vite",
		"vue",
		"webpack"
	],
	"homepage": "https://github.com/so1ve/vue.ts/tree/main/packages/language#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/so1ve/vue.ts.git",
		"directory": "/"
	},
	"bugs": {
		"url": "https://github.com/so1ve/vue.ts/issues"
	},
	"license": "MIT",
	"sideEffects": false,
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		},
		"./*": "./*",
		"./astro": {
			"require": "./dist/astro.cjs",
			"import": "./dist/astro.mjs"
		},
		"./esbuild": {
			"require": "./dist/esbuild.cjs",
			"import": "./dist/esbuild.mjs"
		},
		"./nuxt": {
			"require": "./dist/nuxt.cjs",
			"import": "./dist/nuxt.mjs"
		},
		"./rollup": {
			"require": "./dist/rollup.cjs",
			"import": "./dist/rollup.mjs"
		},
		"./rspack": {
			"require": "./dist/rspack.cjs",
			"import": "./dist/rspack.mjs"
		},
		"./types": {
			"require": "./dist/types.cjs",
			"import": "./dist/types.mjs"
		},
		"./vite": {
			"require": "./dist/vite.cjs",
			"import": "./dist/vite.mjs"
		},
		"./webpack": {
			"require": "./dist/webpack.cjs",
			"import": "./dist/webpack.mjs"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*",
				"./dist/index.d.ts"
			]
		}
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "unbuild",
		"prepublishOnly": "nr build",
		"stub": "unbuild --stub"
	},
	"dependencies": {
		"@nuxt/kit": "^3.8.1",
		"@vue.ts/common": "workspace:*",
		"@vue.ts/language": "workspace:*",
		"magic-string": "^0.30.5",
		"unplugin": "^1.5.0"
	},
	"devDependencies": {
		"@nuxt/schema": "^3.8.1",
		"@vue-macros/better-define": "^1.6.12",
		"rollup": "^4.3.0",
		"unbuild": "^2.0.0",
		"vite": "^5.0.2",
		"webpack": "^5.89.0"
	},
	"peerDependencies": {
		"typescript": "^5.2.2",
		"vue": "^3.3.7"
	}
}
