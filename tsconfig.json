{
	"compilerOptions": {
		"target": "ES2018",
		"lib": ["ESNext"],
		"module": "ESNext",
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"strict": true,
		"strictNullChecks": true,
		"esModuleInterop": true,
		"skipDefaultLibCheck": true,
		"skipLibCheck": true,
		"paths": {
			"@vue.ts/language": ["./packages/language/src/index.ts"]
		}
	}
}
